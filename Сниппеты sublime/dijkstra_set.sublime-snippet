<snippet>
    <content><![CDATA[
st.insert({0, s});
while (!st.empty()) {
    auto it = st.begin();
    int v = (*it).se;
    st.erase(it);
    for (pii p: rb[v]) {
        int ne = p.first;
        int c = p.second;
        if (dist[ne] > dist[v] + c) {
            dist[ne] = dist[v] + c;
            st.insert({dist[ne], ne});
        }
    }
}
    ]]></content>
